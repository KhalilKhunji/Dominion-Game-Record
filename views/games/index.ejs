<%- include('../partials/_header.ejs') %>
    <title>Game Record Index</title>
</head>
<body>
    <h1><%= user.username %>'s Dominion Game Records</h1>
    <a href="/games/new">Add New Record</a>
    <ol>
      <% games.forEach(game => { %>
        <li><a href="/games/<%= game._id %>">Game ID: <%= game._id %></a></li>
      <% }); %>
    </ol>
    <a href="/auth/sign-out">Sign out</a>
</body>
</html>